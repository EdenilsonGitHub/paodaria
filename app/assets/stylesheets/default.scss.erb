.conteudo {
    width: 90%;
    margin: 0 auto;
    margin-bottom: 40px!important;
    margin-top: 80px!important;
    height: 400px;
    background-color: #f5f4ce;
    border-radius: 5px;
    border: 2px solid #53060688;
}

.text-center {
    text-align: center;
}

.div-cabeca {
    background-color: #530606f5;
    width: 100%;
    padding-top: 5px;
    padding-bottom: 5px;
}

.div-cabeca-index {
    width: 100%;
    padding-bottom: 5px;
    background-color: #530606f5;
    height: 53px;
}

.botoes-org {
    display: inline-block;
    float: right;
    height: 42px;
    width: 277px;
    margin-top: 2px;
}

.botoes-org-index {
    display: inline-block;
    float: right;
    height: 42px;
    width: 134px;
    margin-top: 7px;
}

.label-org {
    display: inline-block;
    height: 42px;
    font-size: 23px;
    font-weight: bold;
    padding-top: 5px;
    padding-left: 15px;
    border-top-left-radius: 5px;
    color: white;
    padding-bottom: 30px;
}

.label-org-index {
    display: inline-block;
    height: 42px;
    font-size: 25px;
    padding-left: 35px;
    border-top-left-radius: 5px;
    color: white;
    padding-bottom: 30px;
    font-size: 35px;
}

.nao_ha_registros {
    padding-top: 10px;
    color: red;
}