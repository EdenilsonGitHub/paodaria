.conteudo {
    width: 90%;
    margin: 0 auto;
    margin-top: 80px!important;
    margin-bottom: 40px!important;
    height: 400px;
    background-color: #f5f4ce;
    border-radius: 5px;
    border: 2px solid #53060688;
}

.text-center {
    text-align: center;
}

.div-cabeca {
    background-color: #530606f5;
    width: 100%;
    padding-top: 5px;
    padding-bottom: 5px;
}

.botoes-org {
    display: inline-block;
    float: right;
    height: 42px;
    width: 17%;
    margin-top: 2px;
}

.label-org{
    display: inline-block;
    height: 42px;
    font-size: 20px;
    font-weight: bold;
    padding-top: 5px;
    padding-left: 15px;
    border-top-left-radius: 5px;
    color: white;
    padding-bottom: 30px;
}