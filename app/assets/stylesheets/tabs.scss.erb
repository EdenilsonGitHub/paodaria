/*     Alinhamentos      */

div.tab_container ul.tabs li.current {
    background-color: #920f0f!important;
    color: white;
    font-weight: bold;
}

div.tab_container ul.tabs li {
    margin-top: 15px;
    border:1px solid #920f0f88;
    background-color: #f5f4ce !important;;
    color: #920f0f;
    border-bottom: none;
    display: inline-block;
    padding: 10px 15px;
    cursor: pointer;
}

div.tab_container ul.tabs {
    margin: 0px;
    padding: 0px;
    list-style: none;
}

div.tab_container {
    display: inline-block;
}

